<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tangbaobao.dao.ProductDao">
    <select id="findAllProduct" resultType="product">
        SELECT
            pri_id,
            product_name,
            pro_status
        FROM
            product
        LIMIT 0,3
    </select>
</mapper>